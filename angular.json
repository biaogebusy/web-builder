{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "xinshi": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "server": "src/main.server.ts",
            "prerender": false,
            "ssr": {
              "entry": "server.ts"
            },
            "tsConfig": "tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "src/assets/",
                "output": "/assets/"
              },
              {
                "glob": "favicon.ico",
                "input": "src/",
                "output": "/"
              },
              {
                "glob": "mdi.svg",
                "input": "node_modules/@mdi/angular-material",
                "output": "/assets"
              },
              {
                "glob": "**/*",
                "input": "node_modules/monaco-editor",
                "output": "assets/monaco"
              }
            ],
            "styles": [
              "src/styles.scss",
              "node_modules/swiper/swiper-bundle.css",
              "node_modules/social-share.js/dist/css/share.min.css",
              "node_modules/video.js/dist/video-js.min.css",
              "node_modules/lightgallery/css/lightgallery-bundle.min.css",
              "node_modules/material-design-icons/iconfont/material-icons.css",
              "node_modules/quill/dist/quill.core.css",
              "node_modules/quill/dist/quill.snow.css",
              "node_modules/jsoneditor/dist/jsoneditor.min.css",
              {
                "input": "src/theme/theme-config/blue-config.scss",
                "bundleName": "blue-theme"
              },
              {
                "input": "src/theme/theme-config/dark-config.scss",
                "bundleName": "dark-theme"
              },
              {
                "input": "src/theme/theme-config/green-config.scss",
                "bundleName": "green-theme"
              },
              {
                "input": "src/theme/theme-config/pink-config.scss",
                "bundleName": "pink-theme"
              }
            ],
            "scripts": [
              "node_modules/social-share.js/src/js/qrcode.js",
              "node_modules/social-share.js/src/js/social-share.js"
            ],
            "allowedCommonJsDependencies": [
              "highlight.js",
              "lodash",
              "crypto-js",
              "jsoneditor",
              "quill",
              "dompurify"
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["."]
            }
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "3mb",
                  "maximumError": "8mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "20kb",
                  "maximumError": "32kb"
                }
              ]
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": false
            },
            "nnit": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.nnit.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "3mb",
                  "maximumError": "8mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "20kb",
                  "maximumError": "32kb"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "proxyConfig": "config/proxy.config.js",
            "buildTarget": "xinshi:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "xinshi:build:production"
            },
            "development": {
              "buildTarget": "xinshi:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "xinshi:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.scss"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json",
              "tsconfig.server.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "xinshi:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "xinshi:serve:production"
            }
          }
        },
        "storybook": {
          "builder": "@storybook/angular:start-storybook",
          "options": {
            "configDir": ".storybook",
            "browserTarget": "xinshi:build:development",
            "compodoc": true,
            "compodocArgs": ["-e", "json", "-d", "."],
            "port": 6006
          }
        },
        "build-storybook": {
          "builder": "@storybook/angular:build-storybook",
          "options": {
            "configDir": ".storybook",
            "browserTarget": "xinshi:build:production",
            "compodoc": true,
            "compodocArgs": ["-e", "json", "-d", "."],
            "outputDir": "storybook-static"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "8ba1dbea-2b5b-4c34-a1c7-e926be2d5a75"
  }
}

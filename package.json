    "ng": "ng",
    "start": "ng serve",
    "start:ssl": "ng serve --ssl",
    "start-hmr": "ng serve --hmr -o",
    "build": "ng build --configuration production",
    "build:xinshi": "ng build --configuration xinshi --output-path dist/xinshi/browser && ng run xinshi:server:xinshi --output-path dist/xinshi/server",
    "build:nnit": "ng build --configuration nnit --output-path dist/nnit/browser && ng run xinshi:server:nnit --output-path dist/nnit/server",
    "build:sanziqi": "ng build --configuration sanziqi --output-path dist/sanziqi/browser && ng run xinshi:server:sanziqi --output-path dist/sanziqi/server",
    "build:master": "npm run build:xinshi && npm run build:nnit && npm run build:sanziqi",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run xinshi:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --configuration production && ng run xinshi:server:production",
    "prerender": "ng run xinshi:prerender",
    "bundle-report": "ng build --configuration production --stats-json && webpack-bundle-analyzer dist/browser/stats.json",
    "storybook": "ng run xinshi:storybook",
    "build-storybook": "ng run xinshi:build-storybook"
  },

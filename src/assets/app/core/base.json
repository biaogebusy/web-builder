    "userIdGetPath": "/api/v1",
    "nodeGetPath": "/api/v1/node",
    "commentGetPath": "/api/v1/comment",
    "taxonomyGetPath": "/api/v1/taxonomy_term",
    "flaggingGetPath": "/api/v1/flagging",
    "userGetPath": "/api/v1/user/user",
    "github": {
      "repos": "https://github.com/biaogebusy/xinshi-angular",
      "api": "https://api.github.com/repos/biaogebusy/xinshi-angular"
    }
  },
  "loading": true,
  "article": {
    "fontSize": {
      "enable": true,
      "form": [{
        "label": "字体大小",
        "key": "font",
        "value": 16,
        "type": "input",
        "placeholder": "Ex. 12",
        "controlType": "number",
        "appearance": "legacy",
        "errorMes": "最小: 10px",
        "tooltip": "通过键盘上下键可快速更改大小",
        "matTooltipPosition": "above",
        "suffix": "px"
      }]
    },
    "comment": {
      "enable": true
    }
  },
  "editor": {
    "modules": {
      "toolbar": [
        [{
          "header": [1, 2, 3, 4, 5, 6, false]
        }],
        ["bold", "italic", "underline", "strike"],
        ["clean"],

        [{
          "background": []
        }, {
          "color": []
        }],
        [{
          "align": []
        }],
        [{
          "list": "ordered"
        }, {
          "list": "bullet"
        }],
        [{
          "indent": "-1"
        }, {
          "indent": "+1"
        }],
        ["link"],
        ["blockquote", "code-block"]
      ]
    }
  },
  "actions": {
    "flag": {
      "icon": {
        "name": "star",
        "inline": true
      },
      "enable": true
    },
    "share": {
      "button": {
        "icon": "share",
        "label": "分享"
      },
      "enable": true
    },
    "download": {
      "label": "下载",
      "icon": {
        "name": "file_download",
        "inline": true
      },
      "enable": true
    }
  },
  "dialog": {
    "forceDialogBak": {
      "params": {
        "width": "500px",
        "path": "home",
        "first": true
      },
      "text": {
        "title": {
          "label": "WARNING",
          "style": "style-v1"
        },
        "spacer": "none",
        "body": "Nam liber tempor cum soluta nobis eleifend option congue nihil imperdiet doming id quod mazim placerat facer possim assum. Lorem ipsum dolor sit amet, consectetuer adipiscing elit, sed diam nonummy nibh euismod tincidunt ut laoreet dolore magna aliquam erat volutpat. ",
        "actions": [{
            "type": "closeDialog",
            "label": "21+",
            "color": "primary"
          },
          {
            "type": "link",
            "label": "UNDER 21",
            "href": "http://www.google.com",
            "color": "warn"
          }
        ],
        "actionsAlign": "center center"
      }
    }
  },
  "user": {
    "banner": "/assets/images/16-9/business-14.jpeg"
  },

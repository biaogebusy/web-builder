{
  "title": "View list",
  "body": [{
    "type": "view-list",
    "spacer": "none",
    "params": {
      "apiType": "tab/case/parties-info"
    },
    "tableParams": {
      "sticky": true,
      "style": {
        "height": "500px"
      }
    },
    "tableClasses": "nowrap",
    "header": [{
        "label": "优先等级",
        "key": "transaction_level"
      },
      {
        "label": "案件标题",
        "key": "title"
      },
      {
        "label": "业务时间",
        "key": "business_date"
      },
      {
        "label": "客户名称",
        "key": "customer"
      },
      {
        "label": "经办人员",
        "key": "handler"
      },
      {
        "label": "业务对方",
        "key": "other_party"
      },
      {
        "label": " 法院名称",
        "key": "court"
      },
      {
        "label": "案件阶段",
        "key": "case_procedure"
      },
      {
        "label": "律师",
        "key": "uid"
      }
    ],
    "form": [{
        "type": "input",
        "key": "title",
        "templateOptions": {
          "appearance": "standard",
          "label": "标题"
        }
      },
      {
        "type": "ng-select",
        "key": "customer",
        "api": "/api/v1/filter/customer",
        "none": {
          "label": "无"
        },
        "templateOptions": {
          "multiple": true,
          "hideSelected": true,
          "label": "客户名称"
        }
      },
      {
        "type": "ng-select",
        "key": "handler",
        "search": true,
        "api": "/api/v1/filter/handler",
        "templateOptions": {
          "multiple": true,
          "appearance": "standard",
          "label": "经办人"
        }
      },
      {
        "type": "ng-select",
        "key": "other_party",
        "search": true,
        "api": "/api/v1/filter/other_party",
        "templateOptions": {
          "multiple": true,
          "search": true,
          "appearance": "standard",
          "label": "业务对方"
        }
      },
      {
        "type": "ng-select",
        "key": "court",
        "search": true,
        "api": "/api/v1/filter/court",
        "templateOptions": {
          "multiple": true,
          "search": true,
          "appearance": "standard",
          "label": "法院"
        }
      },
      {
        "type": "ng-select",
        "key": "judge",
        "search": true,
        "api": "/api/v1/filter/judge",
        "templateOptions": {
          "multiple": true,
          "search": true,
          "appearance": "standard",
          "label": "法官"
        }
      },
      {
        "type": "ng-select",
        "key": "case_procedure",
        "search": true,
        "api": "/api/v1/filter/taxonomy_term/case_procedure",
        "templateOptions": {
          "multiple": true,
          "search": true,
          "appearance": "standard",
          "label": "案件阶段"
        }
      }, {
        "type": "date-range",
        "key": "date",
        "templateOptions": {
          "label": "期间",
          "value": "",
          "range": true,
          "appearance": "standard",
          "placeholder": "请选择日期"
        }
      }
    ]
  }]
}

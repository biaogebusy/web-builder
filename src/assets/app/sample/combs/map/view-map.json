{
  "title": "消费者分布",
  "body": [
    {
      "type": "view-map",
      "params": {
        "api": "/api/v2/trade-order/merchant-pay",
        "city": "南宁市",
        "drawer": true
      },
      "form": [
        {
          "type": "mat-select",
          "key": "advert",
          "className": "m-right-xs width-20",
          "api": "/api/v2/filter/advert",
          "templateOptions": {
            "multiple": false,
            "search": true,
            "hideSelected": true,
            "label": "选择广告"
          }
        },
        {
          "type": "mat-select",
          "key": "shop",
          "className": "m-right-xs width-20",
          "api": "/api/v2/filter/shop",
          "templateOptions": {
            "multiple": false,
            "search": true,
            "hideSelected": true,
            "label": "选择门店"
          }
        },
        {
          "type": "date-range",
          "key": "date",
          "className": "width-50",
          "templateOptions": {
            "label": "日期范围",
            "value": "",
            "placeholder": "请选择日期"
          },
          "fieldGroup": [
            {
              "type": "input",
              "key": "start",
              "placeholder": "开始"
            },
            {
              "type": "input",
              "key": "end",
              "placeholder": "结束"
            }
          ]
        }
      ]
    }
  ]
}
